        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/lib-generis-search.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/jig.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/composer-npm-bridge.git"
        }                        
    ],	        
    "require": {
        "openthinklabs/oatbox-extension-installer": "~1.1||dev-master",
        "openthinklabs/tao-core": "48.75.0.6",
        "openthinklabs/generis": "15.13.1.4",
        "openthinklabs/extension-tao-backoffice": ">=6.0.0"
